{
    "name": "symfony/framework-standard-edition",
    "license": "MIT",
    "type": "project",
    "description": "The \"Symfony Standard Edition\" distribution",
    "autoload": {
        "psr-0": { "": "src/", "SymfonyStandard": "app/" }
    },
    "require": {
        "php": ">=5.3.3",
        "symfony/symfony": "2.7.*",
        "doctrine/orm": "~2.2,>=2.2.3",
        "doctrine/doctrine-bundle": "~1.2",
        "twig/extensions": "~1.0",
        "symfony/assetic-bundle": "~2.3",
        "symfony/swiftmailer-bundle": "~2.3",
        "symfony/monolog-bundle": "~2.4",
        "sensio/distribution-bundle": "~3.0",
        "sensio/framework-extra-bundle": "~3.0",
        "incenteev/composer-parameter-handler": "~2.0",

        "sonata-project/easy-extends-bundle": "2.1.*@dev",

        "friendsofsymfony/user-bundle": "2.0.*@dev",
        "pugx/multi-user-bundle": "3.0.*@dev",
        "craue/formflow-bundle": "~2.1",
        "knplabs/knp-menu": "~2.0",
        "knplabs/knp-menu-bundle": "~2.0",
        "sonata-project/media-bundle": "~2.3",
        "jms/serializer-bundle": "~0.11",
        "stof/doctrine-extensions-bundle": "~1.1@dev",
        "sonata-project/intl-bundle": "~2.2",
        "ircmaxell/password-compat": "~1.0",
        "facebook/php-sdk-v4" : "4.0.*",
        "sonata-project/admin-bundle": "^2.3",
        "sonata-project/doctrine-orm-admin-bundle": "^2.3",
        "knplabs/knp-paginator-bundle": "^2.4",
        "beberlei/DoctrineExtensions": "^1.0",
        "friendsofsymfony/jsrouting-bundle": "^1.5"
    },
    "require-dev": {
        "sensio/generator-bundle": "~2.3",
        "behat/behat": "~3.0",
        "behat/mink": "1.7.*@dev",
        "behat/mink-extension": "2.0.*@dev",
        "behat/mink-browserkit-driver": "~1.1",
        "behat/symfony2-extension": "~2.0",
        "behat/mink-goutte-driver": "*",
        "behat/mink-selenium2-driver": "*",
        "ocramius/proxy-manager": "~0.5",
        "phpspec/phpspec": "~2.0",
        "bossa/phpspec2-expect": "~1.0",
        "fabpot/goutte": "2.0.*@dev",
        "sensiolabs/behat-page-object-extension": "2.0.*@dev",
        "squizlabs/php_codesniffer": "1.*",
        "phpmd/phpmd": "2.*",
        "sebastian/phpcpd": "2.*",
        "pdepend/pdepend": "2.*",
        "phploc/phploc": "2.*",
        "theseer/phpdox": "0.7.*",
        "jakoch/phantomjs-installer": "~1.9.8",
        "phpunit/phpunit": "3.7.*",
        "covex-nn/phpcb": "1.*",
        "coduo/php-matcher": "~1.0",
        "doctrine/doctrine-fixtures-bundle": "2.2.*",
        "pawel-grzona/teamcity-phpspec-extension": "2.0.6",
        "anho/behat-formatter-teamcity": "dev-master",
        "hautelook/alice-bundle": "0.2.*"
    },
    "scripts": {
        "post-root-package-install": [
            "SymfonyStandard\\Composer::hookRootPackageInstall"
        ],
        "post-install-cmd": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::removeSymfonyStandardFiles",
            "PhantomInstaller\\Installer::installPhantomJS"
        ],
        "post-update-cmd": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::removeSymfonyStandardFiles",
            "PhantomInstaller\\Installer::installPhantomJS"
        ]
    },
    "config": {
        "bin-dir": "bin"
    },
    "extra": {
        "symfony-app-dir": "app",
        "symfony-web-dir": "web",
        "incenteev-parameters": {
            "file": "app/config/parameters.yml"
        },
        "branch-alias": {
            "dev-master": "2.7-dev"
        },
        "symfony-assets-install": "symlink"
    }
}
